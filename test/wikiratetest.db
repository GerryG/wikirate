n  line-height: 0;\r\n\r\n}\r\na {\r\n  color: $anchor_color;\r\n\r\n}\r\na.wanted-card {\r\n  color: $wanted-card-color;\r\n\r\n}\r\n/* many sites may want this external link behavior; it\'s here because it\'s a nuisance to override when not desired */\r\na.external-link,\r\n.closed-content a.external-link {\r\n  color: $closed-content-a-color;\r\n  background: url(\"/assets/external_link.png\") center right no-repeat;\r\n  padding-right: 13px !important;\r\n\r\n}\r\nimg {\r\n  border: 0;\r\n}\r\nh1,\r\nh2 {\r\n  color: $heading_color;\r\n\r\n}\r\n// h1 {\r\n//   font-size: 1.2em;\r\n//   font-weight: normal;\r\n//   line-height: 1.8em;\r\n//   padding: 5px 0;\r\n\r\n// }\r\n.more-link {\r\n  float: right;\r\n\r\n}\r\n.card-header {\r\n  //font-family: \'Droid Sans\';\r\n}\r\n.card-content {\r\n  .titled-view, .titled_with_edits-view {\r\n    > .card-header {\r\n      line-height: 1.4em;\r\n      margin-top: 1em;\r\n      }\r\n    \r\n  }\r\n  \r\n}\r\n\r\n//Should be moved to Style:Wikirate Metrics once the css are organized \r\n.yinyang {\r\n  float:left;\r\n  width:100%;\r\n}\r\n\r\n#main>.TYPE-analysis>.card-frame>.card-body {\r\n  //padding-top: 0px;\r\n  //margin-top: -1px;\r\n}\r\n//end\r\n\r\n#main > .card-frame {\r\n  border-radius: 4px;\r\n  border: 0 solid #e5e5e5;\r\n  > .card-header {\r\n    background: $card-header-color;\r\n    font-size: 1.1em;\r\n    // font-weight: bold;\r\n    @include card-txt-color;\r\n    border:0;\r\n  }\r\n  > .instruction {\r\n    background: #bbb;\r\n  }\r\n\r\n}\r\n.RIGHT-logo.card-content img {\r\n  max-height: 100%;\r\n\r\n}\r\n#Home {\r\n  overflow: inherit;\r\n}\r\n#main {\r\n  font-size: 1.1em;\r\n  clear: both;\r\n  padding:80px 1% 0px;\r\n  max-width: 1260px;\r\n  margin: 0 auto;\r\n  >.card-frame {\r\n    border-radius: 0;\r\n    > .card-body {\r\n      padding: 10px 2%;\r\n      > .card-header-title {\r\n        font-size: 2em;\r\n        font-weight: normal;\r\n        line-height: 2em;\r\n        padding: 5px 0