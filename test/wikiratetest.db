i\").addClass(\"active\")\r\n    showFakeLoader()\r\n    $(\"#fakeLoader\").hide()\r\n      \r\n  slot.find(\".company-tabs .nav-tabs li a\").off(\"click\").click (e) ->\r\n    if (e.shiftKey || e.ctrlKey || e.metaKey) \r\n      return true\r\n    slot.find(\".company-tabs .nav-tabs li\").removeClass(\"active\")\r\n    $(this).closest(\"li\").addClass(\"active\")\r\n    \r\n    $this = $(this)\r\n    href = $this.data(\'tab-content-url\')\r\n    loadurl = href + \'?view=content\'\r\n    targ = \'.tab-content\'\r\n    if href == undefined\r\n      return true\r\n    $(\'#fakeLoader\').fadeIn()\r\n    History.pushState(null, null, window.location.pathname+\"?tab=\"+$this.data(\"tab-name\"))\r\n    $.get loadurl, (data) ->\r\n      $(targ).html data\r\n      $(targ).trigger(\'slotReady\')\r\n      $(\'#fakeLoader\').fadeOut()\r\n      return\r\n    false\r\n'),(2045,2044,'2016-04-12 23:49:06','2016-04-12 23:49:06',NULL,'script company page+*machine input',1,1,1054,'script_company_page+*machine_input',0,NULL,NULL,'*all plus',500,47,'[[script company page]]'),(2046,NULL,'2016-04-12 23:49:06','2016-04-12 23:49:06',NULL,'note citation script',1,1,NULL,'note_citation_script',0,NULL,NULL,'*all',500,1034,'\r\nvar ajaxFail = function ($popupWindow,xhr, ajaxOptions, thrownError){\r\n\r\n  var html = $(xhr.responseText);\r\n  $popupWindow.html(html); \r\n}\r\n\r\nvar relatedOverviewReturned = function($popupWindow,data){\r\n  //get the topic list\r\n\r\n  $popupWindow.html(data); \r\n  $popupWindow.trigger(\'slotReady\');\r\n}\r\n\r\nvar citationBoxOnClick = function(){\r\n  var $_this = $(this);\r\n  var noteName = $_this.attr(\"id\").split(\"+\")[0];\r\n\r\n  //loading image \r\n  var loadingImageUrl = \"{{loading gif|source;size:medium}}\";\r\n\r\n  //create pop up\r\n  var popupId = \'citation-popup-window\';\r\n  var $popupWindow = $(\'#\'+popupId);\r\n  if($popupWindow.length==0){\r\n    $(\"#main\").prepend(\'<div id=\"\'+popupId+\'\" style=\"display:none;\"></div>\');//c